<!-- DataTales Example -->
<div class="card shadow mb-4">
    <div class="card-header py-3">
      <h6 class="m-0 font-weight-bold text-primary">DataTables Example</h6>
    </div>
    <div class="card-body">
      <div class="table-responsive">
        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
          <thead>
            <tr>
                <th>No</th>
                <th>Date</th>
                <th>Invoice</th>
                <th>Title</th>
                <th>Name Member</th>
                <th>Status</th>
                <th>Action</th>
            </tr>
          </thead>
          <tbody>
            <% var a = 1; bookings.forEach(booking => { %>
              <tr>
                <td><%= a++ %> </td>
                <td><%= booking.bookingStartDate.toLocaleString().split(',')[0] %> s/d <%= booking.bookingEndDate.toLocaleString().split(',')[0] %></td>
                <td><%= booking.invoice %> </td>
                <td><%= booking.itemId.title %> </td>
                <td><%= booking.memberId.firstName %> <%= booking.memberId.lastName %> </td>
                <td><%= booking.payments.status %> </td>
                <td>
                    <a href="/admin/booking/<%= booking.id %>" class="btn btn-success btn-circle btn-sm btn-update mx-2"><i class="fas fa-eye"></i></a>
                </td>
              </tr>
            <% }) %>
          </tbody>
          
        </table>
      </div>
    </div>
  </div>