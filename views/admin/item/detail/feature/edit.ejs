
<!-- Modal -->
<div class="modal fade" id="edit" tabindex="-1" aria-labelledby="editLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="editLabel">Edit Feature</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <form method="POST" action="/admin/item/feature/edit?_method=PUT"  enctype="multipart/form-data">
          <div class="modal-body">
            <div class="form-group">
              <label for="name">Name</label>
              <input type="text" name="name" id="name" class="form-control name" required  placeholder="Enter Name">
            </div>
            <div class="form-group">
              <label for="qty">Qty</label>
              <input type="text" name="qty" id="qty" class="form-control qty" required  placeholder="Qty">
            </div>
            <div class="form-group">
              <label for="name">Image</label>
              <input type="file" name="image" id="name" class="form-control" >
              <span>Pilih ganbar untuk update image</span>
            </div>
          </div>
          <div class="modal-footer">
            <input type="hidden" name="id" class="id">
            <input type="hidden" name="itemId" value="<%= itemId %>">
            <button type="submit" class="btn btn-primary">Edit Feature</button>
          </div>
        </form>
      </div>
    </div>
  </div>